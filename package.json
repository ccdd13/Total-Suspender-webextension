{
  "name": "Total-Suspender-webextension",
  "version": "3.7.0",
  "description": "A minimalistic web extension for firefox that suspends natively (with tabs.discard) not-active tabs.",
  "homepage": "https://github.com/k5md/Total-Suspender-webextension",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "web-ext lint --config .web-ext-config.js",
    "build-dev": "npx rimraf ./dist && npx webpack-cli --config ./webpack.dev.js",
    "build-prod": "npx rimraf ./dist && npx webpack-cli --config ./webpack.prod.js",
    "browser": "web-ext run --config .web-ext-config.js",
    "pack": "npm run build-prod && web-ext build --config .web-ext-config.js",
    "watch": "npm run build-dev && concurrently \"npx webpack-cli --config ./webpack.dev.js --watch\" \"web-ext run --config .web-ext-config.js\"",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "jest": {
    "setupFiles": [
      "jest-webextension-mock"
    ],
    "testURL": "http://localhost/"
  },
  "author": "k5md",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-jsx": "^2.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "concurrently": "^6.0.0",
    "copy-webpack-plugin": "^8.1.0",
    "css-loader": "^5.2.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-jsx": "0.1.0",
    "extract-loader": "^5.1.0",
    "extricate-loader": "^3.0.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "imports-loader": "^2.0.0",
    "interpolate-loader": "^2.0.0",
    "jest": "^26.6.3",
    "jest-webextension-mock": "^3.5.0",
    "node-sass": "^5.0.0",
    "postcss-loader": "^5.2.0",
    "precss": "^4.0.0",
    "prop-loader": "^1.0.0",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "sass-loader": "^11.0.1",
    "spawn-loader": "^7.0.1",
    "style-loader": "^2.0.0",
    "web-ext": "^6.0.0",
    "webextension-polyfill": "^0.7.0",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.3.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "jquery": "^3.5.0",
    "lodash": "^4.17.21",
    "melanke-watchjs": "^1.5.0",
    "mithril": "^2.0.4",
    "popper.js": "^1.15.0",
    "snyk": "^1.461.0",
    "webpack": "^5.28.0",
    "webpack-merge": "^5.7.3"
  },
  "snyk": true
}
